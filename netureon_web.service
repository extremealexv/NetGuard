[Unit]
Description=Netureon Web Interface
After=network.target postgresql.service

[Service]
Type=simple
User=orangepi
WorkingDirectory=/home/orangepi/Netureon
Environment=PYTHONPATH=/home/orangepi/Netureon
ExecStart=/home/orangepi/Netureon/.venv/bin/python webui/app.py
Restart=always
RestartSec=1

[Install]
WantedBy=multi-user.target
